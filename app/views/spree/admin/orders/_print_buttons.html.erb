<% if @order and @order.completed_at? %>
  <br/>
  <center>
    <% if @order.respond_to?(:is_quote) and @order.is_quote %>
      <%= button_link_to("Print Quote", spree.admin_order_path(@order, :pdf , :template => "quote")) %>
    <% else %>
      <%= button_link_to("Print Invoice", spree.admin_order_path(@order, :pdf , :template => "invoice", :balance_due => true)) %>
    <% end %>

  </center>
  <br/><br/><br/>
<% end %>  
